<% provide(:title, 'ユーザー一覧') %>

<h1>登録ユーザー一覧</h1>
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <ul class="users">
      <% @users.each do |user| %>
        <li class="user">
          <%= user.name %><br>id: <%= user.id %><br>登録企業数： <%= user.corporates.count %>
        </li>
      <% end %>
    </ul>
  </div>
</div>